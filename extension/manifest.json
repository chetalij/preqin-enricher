{
  "manifest_version": 3,
  "name": "Preqin Enricher",
  "version": "1.0.0",
  "description": "Enriches HQ details in Preqin CRM using a local FastAPI backend.",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "http://127.0.0.1:8000/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Preqin Enricher"
  }
}
